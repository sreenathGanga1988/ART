@model IEnumerable<ArtWebApp.DataModels.RackMaster>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/ArtMVC/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Rack_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rack_type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddedBy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LocationMaster.LocationName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Rack_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rack_type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddedBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LocationMaster.LocationName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Rack_pk }) |
            @Html.ActionLink("Details", "Details", new { id=item.Rack_pk }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Rack_pk })
        </td>
    </tr>
}

</table>
